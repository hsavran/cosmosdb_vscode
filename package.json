{
	"name": "cosmosdb",
	"displayName": "Azure Cosmos DB",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.60.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:cosmosdb.openEditor"
	],
	"main": "./extension.js",
	"contributes": {
		"customEditors": [
			{
				"viewType": "cosmosdb.sqleditor",
				"displayName": "Cosmos DB SQL Editor",
				"selector": [
					{
						"filenamePattern": "*.cosmos"
					}
				]
			}
		],
		"commands": [
			{
				"command": "cosmosdb.openEditor",
				"title": "Cosmos DB Sql Query"
			}
		]
	},
	"dependencies": {
		"@azure/arm-cosmosdb": "^14.1.1",
		"@azure/arm-resources": "^4.2.2",
		"@azure/arm-subscriptions": "^3.1.2",
		"@azure/core-auth": "^1.3.2",
		"@azure/cosmos": "^3.14.1",
		"@azure/identity": "^1.5.2",
		"@azure/identity-vscode": "^1.0.0-beta.2",
		"@azure/keyvault-secrets": "^4.3.0",
		"@azure/ms-rest-nodeauth": "^3.0.0",
		"azure-arm-cosmosdb": "^2.3.0",
		"json-formatter-js": "^2.3.4"
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.60.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"eslint": "^7.27.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2"
	}
}
