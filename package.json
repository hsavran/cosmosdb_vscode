{
	"name": "cosmosdbsqlapi",
	"displayName": "Cosmos DB SQL Studio",
	"publisher": "SavranWeb",
	"description": "Query Azure Cosmos DB SQL API. Includes Execution Metrics and Indexig Policies.",
	"author": {
		"name": "Hasan Savran"
	},
	"version": "0.0.49",
	"engines": {
		"vscode": "^1.60.0"
	},
	"categories": [
		"Other"
	],
	"icon": "images/logo.png",
	"activationEvents": [
		"onCommand:cosmosdb.openEditor"
	],
	"main": "./extension.js",
	"contributes": {
		"customEditors": [
			{
				"viewType": "cosmosdb.sqleditor",
				"displayName": "Cosmos DB SQL Studio",
				"selector": [
					{
						"filenamePattern": "*.cosmossql"
					}
				]
			}
		],
		"commands": [
			{
				"command": "cosmosdb.openEditor",
				"title": "Cosmos DB Sql Studio"
			}
		]
	},
	"dependencies": {		
		"@azure/arm-cosmosdb": "^14.1.1",
		"@azure/arm-resources": "^4.2.2",
		"@azure/arm-subscriptions": "^3.1.2",
		"@azure/core-auth": "^1.3.2",
		"@azure/cosmos": "^3.15.1",
		"@azure/keyvault-secrets": "^4.3.0",
		"@azure/ms-rest-nodeauth": "^3.1.1",
		"azure-arm-cosmosdb": "^2.3.0",
		"chart.js": "^3.7.1",
		"crypto-js": "^4.1.1",
		"geojson-validation": "^1.0.2",
		"json-formatter-js": "^2.3.4",
		"leaflet": "^1.7.1",
		"leaflet-draw": "^1.0.4",
		"node-fetch": "^2.6.7"
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@azure/identity": "^2.1.0",
		"@azure/identity-vscode": "^1.0.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"@types/vscode": "^1.60.0",
		"eslint": "^7.27.0",
		"glob": "^7.1.7",
		"mocha": "^10.2.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2"
	}
}
